{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "user": {
      "type": "object",
      "properties": {
        "user_id": {
          "type": "integer",
          "minimum": 1
        },
        "email": {
          "type": "string",
          "format": "email"
        },
        "first_name": {
          "type": "string",
          "minLength": 3
        },
        "password": {
          "type": "string",
          "minLength": 8
        },
        "phone_no": {
          "type": "string",
          "minLength": 7
        },
        "last_name": {
          "type": "string",
          "minLength": 3
        }
      },
      "required": ["user_id", "email", "first_name", "password", "phone_no"]
    }
  }
}